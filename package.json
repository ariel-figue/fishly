{
  "name": "fishly",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env DATABASE_URL=$DATABASE_URL_DEV NODE_ENV=development next dev",
    "build": "prisma generate && cross-env NODE_ENV=production next build",
    "start": "cross-env DATABASE_URL=$DATABASE_URL_PRD NODE_ENV=production prisma migrate deploy && next start",
    "postinstall": "prisma generate && prisma migrate deploy",
    "migrate:dev": "cross-env DATABASE_URL=$DATABASE_URL_DEV npx prisma migrate dev",
    "migrate:prod": "cross-env DATABASE_URL=$DATABASE_URL_PRD npx prisma migrate deploy"
  },
